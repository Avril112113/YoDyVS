{
    "name": "yodyvs",
    "displayName": "yodyvs",
    "description": "No Desc",
    "publisher": "Dude112113",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "workspaceContains:save.yodyvs.json"
    ],
    "main": "./out/extension",
    "contributes": {
        "languages": [
			{
				"id": "yolol",
				"extensions": [
                    ".yolol",
                    ".lol",
                    ".yol"
				],
				"aliases": [ "Yolol (YoDyVS)" ],
                "configuration": "./language-configuration.json"
			}
        ],
        "grammars": [
            {
                "language": "yolol",
                "scopeName": "source.yolol",
                "path": "./syntaxes/yolol.tmLanguage.json"
            },
            {
                "scopeName": "markdown.yolol.codeblock",
                "path": "./syntaxes/yolol_md.tmLanguage.json",
				"injectTo": [
					"text.html.markdown"
				],
				"embeddedLanguages": {
					"meta.embedded.block.yolol": "yolol"
				}
            }
        ],
        "commands": [
            {
                "command": "yodyvs.restartWhyBClient",
                "title": "Restart YoDyVS WhyB Client"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "vscode-languageclient": "5.1.1"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
